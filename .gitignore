# Laravel .gitignore

# Dependencies
/node_modules
/public/hot
/public/storage
/storage/*.key
/vendor

# Environment files
.env
.env.backup
.env.production

# Logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
/storage/logs/*.log

# Cache
/bootstrap/cache/*.php
/storage/framework/cache/*
/storage/framework/sessions/*
/storage/framework/views/*

# Compiled assets
/public/build
/public/mix-manifest.json

# IDE files
.vscode/
.idea/
*.swp
*.swo
*~

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Laravel specific
/storage/app/*
!/storage/app/.gitkeep
/storage/framework/*
!/storage/framework/.gitkeep
/storage/logs/*
!/storage/logs/.gitkeep

# Testing
/coverage
/.phpunit.result.cache

# Temporary files
*.tmp
*.temp

# Database
*.sqlite
*.db

# Backup files
*.bak
*.backup

# Custom additions for this project
/public/uploads/*
!/public/uploads/.gitkeep